<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="{{ asset('css/app.css') }}">
    {% block titleContainer %}
    <title>{% block title %}Home{% endblock %} |IOT ENIT</title>
    {% endblock %}
    <link rel="icon" type="image/png" href="{{ asset('img/favicon.png') }}" />


    <!-- font awesome icons -->
    <link rel="stylesheet" href="{{ asset('css/font-awesome.css') }}">

    <!-- Jquery -->
    <script src="{{ asset('js/jquery-1.11.3.min.js') }}"></script>
    <script src="{{ asset('js/jquery-migrate-1.2.1.min.js') }}"></script>

    <!-- scrolling listener -->
    <link rel="stylesheet" href="{{ asset('css/animate.css') }}">

    <!-- Typed.js -->
    <script src="{{ asset('js/typed.js') }}"></script>

    <!-- fonts -->
    <link href='{{ asset('css/titillium.css') }}' rel='stylesheet' type='text/css'>
<link href='https://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet' type='text/css'>

    <link rel="stylesheet" type="text/css" href="{{ asset('css/jquery.datetimepicker.css') }}"/>
    <script src="{{ asset('js/jquery.datetimepicker.full.min.js') }}"></script>

    {% block stylesheets %}{% endblock %}
    <style>
        a.header_nav_list_item, .index_action {
            outline: none;
        }
    </style>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-62600604-3', 'auto');
        ga('send', 'pageview');

    </script>
</head>
<body>
<header class="header header_colored header_flat shaddow">
    <div class="header_title">
        <a href="{{ path('index') }}" style="outline: none;">
            <img src="{{ asset('img/favicon.png') }}" alt="" class="header_title_img">
            <h1 class="header_title_text">IOT PLATFORM <span class="{% block typedScript %}typed{% endblock %} colored">ENIT</span></h1>
        </a>
    </div>
    <nav class="header_nav_list hide-sm animated_header">
        <a class="header_nav_list_item" href="{{ path('promo_list') }}">Promotions</a>
        <a class="header_nav_list_item" href="{{ path('eleves') }}">Students</a>
        <a class="header_nav_list_item" href="{{ path('entreprise_list') }}">Entreprise</a>
        {% if not app.user %}
            <a class="header_nav_list_item" href="{{ path('about') }}">About Us</a>
            <a class="header_nav_list_item" href="{{ path('fos_user_security_login') }}">Log In</a>
        {% else %}
        <div class="header_nav_list_item header_nav_list_submenu">{{ app.user.username }}&nbsp;&nbsp;<i class="fa fa-caret-down header_nav_list_submenu_icon"></i>

            <div class="header_nav_list_submenus">
                <a class="header_nav_list_submenus_item header_nav_list_item" href="{{ path('fos_user_profile_show') }}">Profile</a>
                <a class="header_nav_list_submenus_item header_nav_list_item" href="{{ path('fos_user_security_logout') }}">Logout</a>
            </div>
        </div>
        {% endif %}
    </nav>
    <nav class="header_nav_list header_nav_mobile hide-md  hide-lg">
        <a href="#!" id="header_button" class="header_nav_list_item header_nav_mobile_item"><i class="fa fa-bars"></i></a>
    </nav>
</header>

{% block indexCover %}
{% endblock %}
{% block container %}{% endblock %}

<footer class="footer row">
    <div class="footer_title l4 m4 s12">
        <a href="{{ path('index') }}">
            <img src="{{ asset('/img/favicon.png') }}" alt="" class="footer_title_img">
            <h1 class="footer_title_text">IOT PLATFORM <span class="colored">ENIT</span></h1>
        </a>
        <p class="footer_title_copyright">Â© 2018 <a href="" target="_blank">Mohamed Boubaya</a></p>
        <!--
        <nav class="footer_socials">
          <a href="#" class="footer_socials_item"><i class="fa fa-facebook"></i></a>
          <a href="#" class="footer_socials_item"><i class="fa fa-twitter"></i></a>
          <a href="#" class="footer_socials_item"><i class="fa fa-linkedin"></i></a>
        </nav>
        -->
    </div>
    <nav class="footer_links l4 m4 s12">
        <span class="footer_link_item footer_link_item_header">Menu</span>
        <a class="footer_link_item" href="{{ path('about') }}">About us</a>
        <!--<a class="footer_link_item" href="{{ path('faq') }}">FAQ</a>-->
        <a class="footer_link_item" href="{{ path('faq') }}">FAQ</a>
        <a class="footer_link_item" href="{{ path('contact') }}">Contact us</a>
    </nav>
    <div class="footer_g2foss l4 m4 s12">
        <a href="" target="_blank">
            <img src="{{ asset('img/g2foss.png') }}" alt="" class="footer_g2foss_img">
        </a>
        <h1 class="footer_g2foss_text">Made by <a href="" target="_blank" class="coloredG2foss">Mohamed Boubaya</a></h1>
    </div>
</footer>
<script src="{{ asset('js/toast.js') }}"></script>
{% block javascripts %}
    <script src="{{ asset('js/header.js') }}"></script>
    <script>
        $('.datetime').datetimepicker();
    </script>
    <script>
        $(form).formValidation({
            icon: {
                valid: 'fa fa-check-circle colored',
                invalid: 'fa fa-times red',
                validating: 'fa fa-refresh'
            }
        });
    </script>
{% endblock %}
{% block toasts %}
{% endblock %}
</body>
</html>